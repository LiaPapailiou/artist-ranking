{
  "env": {
    "node": true,
    "browser": true
  },
  "plugins": [
    "jest"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "extends": [
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "linebreak-style": 0,
    "react/jsx-props-no-spreading": "off",
    "react/require-default-props": 0,
    "react/default-props-match-prop-types": 0,
    "react/jsx-one-expression-per-line": "off"
  }
}